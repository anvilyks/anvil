function replaceBlobWithCustomURLInProject(project, blobToCustomMap) {
  console.log("ğŸ”„ Start blobâ†’custom replacement");

  function traverse(obj) {
    if (Array.isArray(obj)) {
      for (const item of obj) {
        traverse(item);
      }
    } else if (obj && typeof obj === "object") {
      // Zamiana: comp.attributes.src
      if (obj.attributes?.src && blobToCustomMap[obj.attributes.src]) {
        console.log("ğŸ” Replacing attributes.src", obj.attributes.src, "â†’", blobToCustomMap[obj.attributes.src]);
        obj.attributes.src = blobToCustomMap[obj.attributes.src];
      }

      // Zamiana: comp.src
      if (obj.src && blobToCustomMap[obj.src]) {
        console.log("ğŸ” Replacing src", obj.src, "â†’", blobToCustomMap[obj.src]);
        obj.src = blobToCustomMap[obj.src];
      }

      // Rekurencja po wszystkich kluczach
      for (const key in obj) {
        traverse(obj[key]);
      }
    }
  }

  // GÅ‚Ã³wna pÄ™tla po stronach i komponentach
  for (const page of project.pages || []) {
    for (const frame of page.frames || []) {
      traverse(frame.component);
    }
  }

  console.log("âœ… Blobâ†’custom replacement completed");
}
